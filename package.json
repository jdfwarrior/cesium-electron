{
  "name": "-cesium-electron",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "start": "cross-env cesiumenv=dev electron .",
    "release:win": "electron-packager ./ --platform=win32 --arch=x64 --executable-name=\"Cesium Viewer\" --extra-resource ./dist --out ./release",
    "release:mac": "electron-packager ./ --platform=darwin --arch=x64 --executable-name=\"Cesium Viewer\" --extra-resource ./dist --out ./release"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "cesium": "^1.101.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "pinia": "^2.0.28",
    "vue": "3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cypress": "^12.3.0",
    "electron": "^22.0.2",
    "electron-packager": "^17.1.1",
    "eslint": "^8.31.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.24"
  }
}
